generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  id_nurse    String   @default(uuid()) @db.Uuid
  image       String?
  first_name  String?
  last_name   String?
  phone       String?
  email       String?  @unique
  gender      Boolean
  agency      String?
  job         String?
  address     String?
  password    String?
  data        Data[]   @relation("UserData")
}

model Data {
  id       Int      @id @default(autoincrement())
  user_id  Int
  time     DateTime
  name     String?
  status   String?
  volume   Float?
  user     User     @relation(fields: [user_id], references: [id], name: "UserData")
}